	{
		"title": "Change options dynamically",
		"html": "<div class=\"btn-group\">\n\t<button class=\"btn btn-default\"\n\t\tng-class=\"{active:zone=='global'}\"\n\t\tng-click=\"zone='global'\">\n\t\tGlobal Rivers\n\t</button>\n\t<button class=\"btn btn-default\"\n\t\tng-class=\"{active:zone=='european'}\"\n\t\tng-click=\"zone='european'\">\n\t\tEuropean Rivers\n\t</button>\n</div>\n\n<select selector\n\tmodel=\"river\"\n\toptions=\"rivers\"\n\tvalue-attr=\"name\"\n\tlabel-attr=\"name\"\n\tplaceholder=\"Select a river...\"></select>\n\n<dl class=\"dl-horizontal\">\n\t<dt>River</dt>\n\t<dd ng-bind=\"river || '-'\"></dd>\n\t\n\t<dt>Length</dt>\n\t<dd ng-bind=\"selectedRivers[river].length\n\t\t\t? (selectedRivers[river].length | number) + ' km'\n\t\t\t: '-'\"></dd>\n\t\n\t<dt>Outflow</dt>\n\t<dd ng-bind=\"selectedRivers[river].outflow || '-'\"></dd>\n</dl>\n\n<p>\n\tCurrent value: <code ng-bind=\"river|json\"></code>\n</p>",
		"js": "$scope.zone = \"global\";\n\n$scope.globalRivers = {\n\t\"Nile\":        { length: 6690, outflow: \"Mediterranean\" },\n\t\"Amazon\":      { length: 6296, outflow: \"Atlantic Ocean\" },\n\t\"Mississippi\": { length: 5970, outflow: \"Gulf of Mexico\" },\n\t\"Yangtze\":     { length: 5797, outflow: \"China Sea\" },\n\t\"Ob\":          { length: 5567, outflow: \"Gulf of Ob\" },\n\t\"Yellow\":      { length: 4667, outflow: \"Gulf of Chihli\" },\n\t\"Yenisei\":     { length: 4506, outflow: \"Arctic Ocean\" },\n\t\"Paraná\":      { length: 4498, outflow: \"Río de la Plata\" },\n\t\"Irtish\":      { length: 4438, outflow: \"Ob River\" },\n\t\"Chambeshi\":   { length: 4371, outflow: \"Atlantic Ocean\" }\n};\n\n$scope.europeanRivers = {\n\t\"Volga\":       { length: 3692, outflow: \"Caspian Sea\" },\n\t\"Danube\":      { length: 2860, outflow: \"Black Sea\" },\n\t\"Ural\":        { length: 2428, outflow: \"Caspian Sea\" },\n\t\"Dnieper\":     { length: 2290, outflow: \"Black Sea\" },\n\t\"Don\":         { length: 1950, outflow: \"Sea of Azov\" },\n\t\"Pechora\":     { length: 1809, outflow: \"Arctic Ocean\" },\n\t\"Kama\":        { length: 1805, outflow: \"Volga\" },\n\t\"Oka\":         { length: 1500, outflow: \"Volga\" },\n\t\"Belaya\":      { length: 1430, outflow: \"Kama\" },\n\t\"Dniester\":    { length: 1362, outflow: \"Black Sea\" }\n};\n\n$scope.$watch('zone', function (zone) {\n\t$scope.rivers = [];\n\t$scope.selectedRivers = $scope[zone + 'Rivers'];\n\tangular.forEach($scope.selectedRivers, function (river, name) {\n\t\t$scope.rivers.push({\n\t\t\tname:    name,\n\t\t\tlength:  river.length,\n\t\t\toutflow: river.outflow\n\t\t});\n\t});\n\t// select first\n\t$scope.river = $scope.rivers[0].name;\n});"
	}
